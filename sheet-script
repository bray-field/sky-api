	let lists = BSky3.getBBLists({
		lists: getLoL()
	}).map(z => ({ ...z,
		value: checkFix(fixComment, fixGrade, getXData(z.value))
	}));
	['bio', 'report_card', 'transcript'].forEach(a => {
		lists.find(z => z.name === a).value = fixRecords[a](findList(lists, a))
	});
